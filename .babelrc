{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": ["add-module-exports", "react-hot-loader/babel"],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ],
      "plugins": [
        "react-hot-loader/babel",
        "transform-decorators-legacy",
        "transform-runtime",
        "add-module-exports",
        "transform-es2015-modules-commonjs",
      ]
    }
  }
}
